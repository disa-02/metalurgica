<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Roow.
    -->
    <changeSet id="20221125150159-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="sale_sale_code"
                                 baseTableName="roow"
                                 constraintName="fk_roow__sale_id"
                                 referencedColumnNames="sale_code"
                                 referencedTableName="sale"/>

        <addForeignKeyConstraint baseColumnNames="product_id"
                                 baseTableName="roow"
                                 constraintName="fk_roow__product_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>
    </changeSet>
</databaseChangeLog>
