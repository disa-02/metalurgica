<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Subscribe.
    -->
    <changeSet id="20221125150156-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="notification_id"
                                 baseTableName="subscribe"
                                 constraintName="fk_subscribe__notification_id"
                                 referencedColumnNames="id"
                                 referencedTableName="notification"/>

        <addForeignKeyConstraint baseColumnNames="operator_id"
                                 baseTableName="subscribe"
                                 constraintName="fk_subscribe__operator_id"
                                 referencedColumnNames="id"
                                 referencedTableName="operator"/>
    </changeSet>
</databaseChangeLog>
